@model Entity.Concerete.CarStatus
@{
    ViewData["Title"] = "Add";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h3>Yeni Araba Durumu Girişi</h3>

<form method="post" asp-controller="CarStatus" asp-action="Add">
    <label>Araba Durumu</label>
    <input type="text" asp-for="CarStatusName" class="form-control" style="border:2px solid #2222" />
    <br />
    <button class="btn btn-primary">Kaydet</button>
</form>

